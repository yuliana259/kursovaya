<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="109.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Report">
   <children>
      <Label layoutX="92.0" layoutY="14.0" text="Отчет по количеству принятых/уволенных сотрудников">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Label layoutX="21.0" layoutY="54.0" text="Выбрать дату">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="date" layoutX="145.0" layoutY="54.0" />
      <Button fx:id="formButton" layoutX="360.0" layoutY="54.0" mnemonicParsing="false" text="Сформировать">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="Назад" />
      <Label fx:id="errorLabel" layoutX="145.0" layoutY="87.0" prefHeight="17.0" prefWidth="174.0" textFill="RED" />
   </children>
</AnchorPane>
